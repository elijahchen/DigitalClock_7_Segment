<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="rectangle">

            <solid
                android:color="#FFFFFF"/>

            <size
                android:height="10dp"
                android:width="10dp"/>
        </shape>
    </item>

    <item>
        <shape android:shape="line">

            <stroke
                android:width="10dp"
                android:color="#ff0000" />

        </shape>
    </item>

</layer-list>